<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="100.0" minWidth="100.0" prefHeight="584.0" prefWidth="598.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="clientModule.controllers.GroupWindowController">
    <left>
        <VBox alignment="CENTER" minHeight="100.0" minWidth="100.0" prefHeight="311.0" prefWidth="200.0" BorderPane.alignment="CENTER">
            <BorderPane.margin>
                <Insets left="50.0" />
            </BorderPane.margin>
            <children>
                <Label fx:id="nameLabel" alignment="CENTER" contentDisplay="CENTER" text="Имя группы" />
                <TextField fx:id="name" VBox.vgrow="ALWAYS" />
                <Label fx:id="coordLabel" text="Координаты" />
                <HBox alignment="CENTER" prefHeight="32.0" prefWidth="200.0" spacing="15.0">
                    <children>
                        <TextField prefHeight="39.0" prefWidth="229.0" fx:id="x" />
                        <TextField fx:id="y" prefHeight="39.0" prefWidth="230.0" />
                    </children>
                </HBox>
                <Label fx:id="stCount" text="Количество студентов" />
                <TextField fx:id="studentsCount" />
                <Label fx:id="stExp" text="Количество должников" />
                <TextField fx:id="studentsShouldBeExpelled" />
                <Label fx:id="avMark" text="Средняя оценка" />
                <TextField fx:id="averageMark" />
                <Label fx:id="form" text="Форма обучения" />
                <ChoiceBox fx:id="formOfEducation" maxHeight="-Infinity" minHeight="-Infinity" prefHeight="27.0" prefWidth="139.0" />
            </children>
        </VBox>
    </left>
    <right>
        <VBox alignment="CENTER" prefHeight="403.0" prefWidth="200.0" spacing="0.5" BorderPane.alignment="CENTER">
            <BorderPane.margin>
                <Insets right="50.0" />
            </BorderPane.margin>
            <children>
            <Label fx:id="admin" text="Админ группы:">
               <font>
                  <Font name="System Bold" size="18.0" />
               </font>
            </Label>
                <Label fx:id="adminN" text="Имя">
                    <VBox.margin>
                        <Insets />
                    </VBox.margin>
                </Label>
                <TextField fx:id="adminName">
                    <VBox.margin>
                        <Insets />
                    </VBox.margin>
                </TextField>
                <Label fx:id="nomPassport" text="Номер паспорта" />
                <TextField fx:id="passportID" />
                <Label fx:id="color" text="Цвет волос" />
            <ChoiceBox fx:id="colorBox" maxHeight="-Infinity" minHeight="-Infinity" prefHeight="27.0" prefWidth="139.0" />
            <Label fx:id="nation" text="Национальность" />
            <ChoiceBox fx:id="Nationality" maxHeight="-Infinity" minHeight="-Infinity" prefHeight="27.0" prefWidth="139.0" />
            <Label fx:id="loc" text="Локация админа:">
               <font>
                  <Font name="System Bold" size="18.0" />
               </font>
            </Label>
            <Label fx:id="locName" text="Название локации:" />
            <TextField fx:id="locationName" />
            <Label fx:id="coordLabell" text="Координаты:" />
            <HBox prefHeight="32.0" prefWidth="200.0" spacing="15.0">
               <children>
                  <TextField prefHeight="200.0" prefWidth="230.0" fx:id="xx" />
                  <TextField fx:id="yy" prefHeight="200.0" prefWidth="230.0" />
                  <TextField fx:id="zz" prefHeight="200.0" prefWidth="230.0" />
               </children>
            </HBox>
            </children>
        </VBox>
    </right>
    <top>
        <Label fx:id="group" text="Группа" BorderPane.alignment="CENTER">
            <font>
                <Font name="System Bold" size="29.0" />
            </font>
            <BorderPane.margin>
                <Insets top="15.0" />
            </BorderPane.margin>
        </Label>
    </top>
    <bottom>
        <HBox alignment="BOTTOM_CENTER" prefHeight="42.0" prefWidth="591.0" spacing="20.0" BorderPane.alignment="CENTER">
            <children>
                <Button fx:id="enter" alignment="CENTER" mnemonicParsing="false" onAction="#enter" text="Ввести" />
                <Button fx:id="back" mnemonicParsing="false" onAction="#back" text="Назад" />
            </children>
         <padding>
            <Insets bottom="5.0" top="2.0" />
         </padding>
         <BorderPane.margin>
            <Insets top="1.0" />
         </BorderPane.margin>
        </HBox>
    </bottom>
</BorderPane>
