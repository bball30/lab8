<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<BorderPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="100.0" minWidth="100.0" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="clientModule.controllers.RegisterWindowController">
    <center>
        <VBox alignment="CENTER" minHeight="100.0" minWidth="100.0" prefHeight="200.0" prefWidth="100.0" spacing="15.0" BorderPane.alignment="CENTER">
            <children>
                <Label fx:id="registrationLabel" text="Регистрация" VBox.vgrow="ALWAYS">
                    <font>
                        <Font name="System Bold" size="28.0" />
                    </font>
                </Label>
                <Label fx:id="loginLabel" text="Логин" />
                <TextField fx:id="login" alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" onAction="#getLogin" prefHeight="31.0" prefWidth="200.0" VBox.vgrow="ALWAYS" />
                <Label fx:id="passwordLabel" text="Пароль" />
                <PasswordField fx:id="password" maxHeight="-Infinity" maxWidth="-Infinity" onAction="#getPassword" prefHeight="31.0" prefWidth="201.0" />
                <Button fx:id="registerButton" mnemonicParsing="false" onAction="#register" text="Зарегистрировать" VBox.vgrow="ALWAYS" />
                <Button fx:id="backButton" mnemonicParsing="false" onAction="#back" text="Назад" />
            </children>
        </VBox>
    </center>
</BorderPane>
